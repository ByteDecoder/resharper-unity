<?xml version="1.0" encoding="utf-8"?>
<Errors language="C#" configurableSeverityImplementationLanguage="CSHARP"
        baseClass="BurstHighlighting">

  <Usings>
    JetBrains.ReSharper.Psi.CSharp;
    JetBrains.ReSharper.Psi.CSharp.Tree;
    JetBrains.ReSharper.Plugins.Unity.CSharp.Daemon.Errors;
    JetBrains.ReSharper.Plugins.Unity.CSharp.Daemon.Stages.Highlightings;
    JetBrains.ReSharper.Plugins.Unity.CSharp.Psi.Resolve;
    JetBrains.ReSharper.Plugins.Unity.CSharp.Daemon.Stages.PerformanceCriticalCodeAnalysis.Highlightings;
  </Usings>

  <SeverityConfiguration>
    <Group name="UnityHighlightingGroupIds.Burst">
      <Tag externalName="BurstUnableToAccessManagedMethodWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Accessing managed method</Title>
        <Description>
          Unable to access the managed method
        </Description>
      </Tag>

      <Tag externalName="BurstTryNotSupportedWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>The 'try' construction is not supported</Title>
        <Description>
          The 'try' construction is not supported
        </Description>
      </Tag>

      <Tag externalName="BurstFunctionSignatureContainsManagedObjectsWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Function signature contains managed objects</Title>
        <Description>
          Function signature contains managed objects
        </Description>
      </Tag>

      <Tag externalName="BurstBoxingNotSupportedWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Boxing a valuetype to a managed object is not supported</Title>
        <Description>
          Boxing a valuetype to a managed object is not supported
        </Description>
      </Tag>

      <Tag externalName="BurstCreatingManagedObjectWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Creating a managed object is not supported</Title>
        <Description>
          Creating a managed object is not supported
        </Description>
      </Tag>

      <Tag externalName="BurstManagedStringLiteralWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Loading a managed string literal is not supported</Title>
        <Description>
          Loading a managed string literal is not supported
        </Description>
      </Tag>

      <Tag externalName="BurstWriteStaticFieldWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Writing to a static field is not supported</Title>
        <Description>
          Writing to a static field is not supported
        </Description>
      </Tag>
      
      <Tag externalName="BurstForeachNotSupportedWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>The 'foreach' construction is not supported</Title>
        <Description>
          The 'foreach' construction is not supported
        </Description>
      </Tag>

      <Tag externalName="BurstLoadingStaticNotReadonlyWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Loading from a non-readonly static field is not supported</Title>
        <Description>
          Loading from a non-readonly static field is not supported
        </Description>
      </Tag>

      <Tag externalName="BurstLoadingManagedObjectWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Loading managed object is not supported</Title>
        <Description>
          Loading managed object is not supported
        </Description>
      </Tag>

      <Tag externalName="BurstDebugLogInvalidArgumentWarning.HIGHLIGHTING_ID" default="WARNING">
        <Title>Invalid argument. Expecting string literal or string.</Title>
        <Description>
          Invalid argument. Expecting string literal or string.
        </Description>
      </Tag>
    </Group>
  </SeverityConfiguration>
  
  <Warning name="BurstUnableToAccessManagedMethod" configurableSeverity="Unity.BurstUnableToAccessManagedMethod">
    <Parameter type="DocumentRange" name="range" />
    <Parameter type="string" name="methodQualification"/>
    <Parameter type="string" name="typeQualification"/>
    <Message value="Unable to access the managed method '{0}' from type '{1}'">
      <Argument>MethodQualification</Argument>
      <Argument>TypeQualification</Argument>
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstTryNotSupported" configurableSeverity="Unity.BurstTryNotSupported">
    <Parameter type="DocumentRange" name="range" />
    <Message value="The 'try' construction is not supported">
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstFunctionSignatureContainsManagedObjects" configurableSeverity="Unity.BurstFunctionSignatureContainsManagedObjects">
    <Parameter type="DocumentRange" name="range" />
    <Parameter type="string" name="functionSignature"/>
    <Message value="The managed function '{0}' signature contains managed objects">
      <Argument>FunctionSignature</Argument>
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstBoxingNotSupported" configurableSeverity="Unity.BurstBoxingNotSupported">
    <Parameter type="DocumentRange" name="range" />
    <Parameter type="string" name="typeQualification"/>
    <Message value="Boxing a value type '{0}' to a managed object is not supported">
      <Argument>TypeQualification</Argument>
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstCreatingManagedObject" configurableSeverity="Unity.BurstCreatingManagedObject">
    <Parameter type="DocumentRange" name="range" />
    <Parameter type="string" name="typeQualification"/>
    <Message value="Creating a managed object '{0}' is not supported">
      <Argument>TypeQualification</Argument>
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstManagedStringLiteral" configurableSeverity="Unity.BurstManagedStringLiteral">
    <Parameter type="DocumentRange" name="range" />
    <Message value="Loading a managed string literal is not supported"/>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstWriteStaticField" configurableSeverity="Unity.BurstWriteStaticField">
    <Parameter type="DocumentRange" name="range" />
    <Parameter type="string" name="fieldQualification"/>
    <Message value="Writing to a static field '{0}' is not supported">
      <Argument>FieldQualification</Argument>
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstForeachNotSupported" configurableSeverity="Unity.BurstForeachNotSupported">
    <Parameter type="DocumentRange" name="range" />
    <Message value="The 'foreach' construction is not supported">
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstLoadingStaticNotReadonly" configurableSeverity="Unity.BurstLoadingStaticNotReadonly">
    <Parameter type="DocumentRange" name="range" />
    <Parameter type="string" name="fieldQualification"/>
    <Message value="Loading from a non-readonly static field '{0}' is not supported">
      <Argument>FieldQualification</Argument>
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstLoadingManagedObject" configurableSeverity="Unity.BurstLoadingManagedObject">
    <Parameter type="DocumentRange" name="range" />
    <Parameter type="string" name="typeQualification"/>
    <Message value="Loading managed class '{0}' is not supported">
      <Argument>TypeQualification</Argument>
    </Message>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>

  <Warning name="BurstDebugLogInvalidArgument" configurableSeverity="Unity.BurstDebugLogInvalidArgument">
    <Parameter type="DocumentRange" name="range" />
    <Message value="Invalid argument. Expecting string literal or string"/>
    <Range>Range</Range>
    <Behavour overlapResolvePolicy="NONE" />
  </Warning>
</Errors>